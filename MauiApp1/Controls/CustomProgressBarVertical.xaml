<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PingWall.Controls.CustomProgressBarVertical">
    <Grid x:Name="MainGrid" >
        <Grid.RowDefinitions>
            <RowDefinition Height="100*"></RowDefinition>
            <RowDefinition Height="100*"></RowDefinition>
        </Grid.RowDefinitions>
        <Frame x:Name="TopFrame" CornerRadius="0" Grid.Row="0" HorizontalOptions="Fill" VerticalOptions="Fill" ></Frame>
        <Frame x:Name="BottomFrame" Grid.Row="1" CornerRadius="0" HorizontalOptions="Fill" VerticalOptions="Fill" ></Frame>
    </Grid>
</ContentView>
